
{
  "inbounds": [{
    "port": 1080,
    "listen": "0.0.0.0",
    "protocol": "socks",
    "settings": {
      "auth": "noauth",
      "udp": true
    }
  }],
  "outbounds": [{
    "protocol": "vmess",
    "settings": {
      "vnext": [{
        "address": "server_address",
        "port": 443,
        "users": [{
          "id": "your_uuid_here",
          "alterId": 64
        }]
      }]
    },
    "streamSettings": {
      "network": "tls"
    }
  }],
  "dns": {
    "servers": ["8.8.8.8", "8.8.4.4"]
  }
}
